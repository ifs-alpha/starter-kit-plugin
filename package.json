{
	"name": "starter-kit-plugin",
	"displayName": "Starter Kit Plugin",
	"description": "Plugin to pre-generate starter kit config",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:starter-kit-plugin.run-starter-config"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "starter-kit-plugin.run-starter-config",
				"title": "Starter Config"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.63.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"generate-template-files": "^3.2.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"zip-a-folder": "^1.1.0"
	}
}
